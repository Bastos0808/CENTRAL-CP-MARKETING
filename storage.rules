
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Permite que qualquer usuário autenticado leia e escreva arquivos
    // Para um ambiente de produção, você pode querer restringir isso mais,
    // por exemplo, permitindo que um usuário escreva apenas em sua própria pasta.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
